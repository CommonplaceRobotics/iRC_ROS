<?xml version="1.0" ?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro"
       name="igus_rebel_6dof" >
  <link name="$(arg prefix)base_link">
    <visual>
      <geometry>
        <mesh filename="package://irc_ros_description/meshes/igus_rebel_6dof_pre/Joint0.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://irc_ros_description/meshes/igus_rebel_6dof_pre/Joint0Coll.dae"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>
  <joint name="$(arg prefix)joint1" type="revolute">
    <parent link="$(arg prefix)base_link"/>
    <child link="$(arg prefix)link_1"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0 0.144"/>
    <axis xyz="0 0 -1"/>
    <limit effort="330.0" lower="${-pi*179/180}" upper="${pi*179/180}" velocity="2.16"/>
  </joint>
  <link name="$(arg prefix)link_1">
    <visual>
      <geometry>
        <mesh filename="package://irc_ros_description/meshes/igus_rebel_6dof_pre/Joint1.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://irc_ros_description/meshes/igus_rebel_6dof_pre/Joint1Coll.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>
  <joint name="$(arg prefix)joint2" type="$(arg prefix)revolute">
    <parent link="$(arg prefix)link_1"/>
    <child link="$(arg prefix)link_2"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0 0.130"/>
    <axis xyz="0 1 0"/>
    <limit effort="330.0" lower="${-pi*2/3 + pi/6}" upper="${pi*2/3 + pi/6}" velocity="2.16"/>
  </joint>
  <link name="$(arg prefix)link_2">
    <visual>
      <geometry>
        <mesh filename="package://irc_ros_description/meshes/igus_rebel_6dof_pre/Joint2.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://irc_ros_description/meshes/igus_rebel_6dof_pre/Joint2Coll.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>
  <joint name="$(arg prefix)joint3" type="revolute">
    <parent link="$(arg prefix)link_2"/>
    <child link="$(arg prefix)link_5"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.2384"/>
    <axis xyz="0 1 0"/>
    <limit effort="330.0" lower="${-pi*2/3 + pi/6}" upper="${pi*2/3 + pi/6}" velocity="2.16"/>
  </joint>
  <link name="$(arg prefix)link_5">
    <visual>
      <geometry>
        <mesh filename="package://irc_ros_description/meshes/igus_rebel_6dof_pre/Joint3.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://irc_ros_description/meshes/igus_rebel_6dof_pre/Joint3Coll.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>
  <joint name="$(arg prefix)joint4" type="revolute">
    <parent link="$(arg prefix)link_5"/>
    <child link="$(arg prefix)link_6"/>
    <!-- FIXME The 0.03m offset are most likely caused by the mesh file -->
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0 ${0.149 - 0.03}"/>
    <axis xyz="0 0 1"/>
    <limit effort="330.0" lower="${-pi*179/180}" upper="${pi*179/180}" velocity="2.16"/>
  </joint>
  <link name="$(arg prefix)link_6">
    <visual>
      <geometry>
        <mesh filename="package://irc_ros_description/meshes/igus_rebel_6dof_pre/Joint4.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://irc_ros_description/meshes/igus_rebel_6dof_pre/Joint4Coll.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>
  <joint name="$(arg prefix)joint5" type="revolute">
    <parent link="$(arg prefix)link_6"/>
    <child link="$(arg prefix)link_7"/>
    <!-- FIXME The 0.03m offset are most likely caused by the mesh file -->
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0 ${0.14 + 0.03}"/>
    <axis xyz="0 1 0"/>
    <limit effort="330.0" lower="${-pi*19/36}" upper="${pi*19/36}" velocity="2.16"/>
  </joint>
  <link name="$(arg prefix)link_7">
    <visual>
      <geometry>
        <mesh filename="package://irc_ros_description/meshes/igus_rebel_6dof_pre/Joint5.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://irc_ros_description/meshes/igus_rebel_6dof_pre/Joint5Coll.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>
  <xacro:if value="$(arg continuous_last_joint)">
     <joint name="$(arg prefix)joint6" type="continuous">
      <parent link="$(arg prefix)link_7"/>
      <child link="$(arg prefix)link_8"/>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 0 0.1208"/>
      <axis xyz="0 0 1"/>
      <limit effort="330.0" velocity="2.16"/>
    </joint> 
  </xacro:if>
  <xacro:unless value="$(arg continuous_last_joint)">
    <joint name="$(arg prefix)joint6" type="revolute">
      <parent link="$(arg prefix)link_7"/>
      <child link="$(arg prefix)link_8"/>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 0 0.1208"/>
      <axis xyz="0 0 1"/>
      <limit effort="330.0" lower="${-pi*179/180}" upper="${pi*179/180}" velocity="2.16"/>
    </joint>
  </xacro:unless>
  <link name="link_8">
    <visual>
      <geometry>
        <mesh filename="package://irc_ros_description/meshes/igus_rebel_6dof_pre/Joint6.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://irc_ros_description/meshes/igus_rebel_6dof_pre/Joint6Coll.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>
  <joint name="$(arg prefix)joint7" type="fixed">
    <parent link="$(arg prefix)link_8"/>
    <child link="$(arg prefix)hand"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0 0.0012"/>
    <axis xyz="0 0 1"/>
    <limit effort="330.0" lower="-3.12414" upper="3.12414" velocity="2.16"/>
  </joint>
  <link name="$(arg prefix)hand">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>
</robot>
